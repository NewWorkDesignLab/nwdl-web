<%= render 'nwdl/shared/resource/index' do %>

  <%# IMAGE GEAR %>
  <% content_for :image_title do %>
    <div class="absolute project-gear -z-10"><%= render 'nwdl/shared/images/zahnrad' %></div>
  <% end %>

  <%# PRESENTER SELF %>
  <% content_for :presenter_content do %>
    <%= render 'nwdl/shared/resource/iterator', resource_name: $spina_projects, nil_message: "Leider ist hier noch nichts zu sehen." do |resource| %>
      <%= render 'nwdl/shared/resource/resource_default', resource: resource %>
    <% end %>
  <% end %>

<% end %>
